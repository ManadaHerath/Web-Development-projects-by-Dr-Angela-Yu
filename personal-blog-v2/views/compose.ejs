<%-include("partials/header")-%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="title">Title</label>
        <input class="form-control" type="text" class="form-control" id = "title" name="postTitle">
        <label for="textArea">Post</label>
        <textarea class="form-control" rows="5" class="form-control" id = "textArea" name="postData"></textarea>
    </div>
    
    <button type="submit" class="btn btn-primary mb-2" name="cbutton">Publish</button>
</form>
<%-include("partials/footer")-%>